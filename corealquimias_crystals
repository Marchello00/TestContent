            $(this).addClass("active").children(".js-desktop-dropdown").stop(true, true).fadeIn(300);
        }, function() {
            $(this).removeClass("active").children(".js-desktop-dropdown").stop(true, true).fadeOut(300);
        });



        
        $(".js-toggle-search").click(function (e) {
            e.preventDefault;
